{
  "name": "suprematism-venn-diagram",
  "version": "0.0.1",
  "license": "MIT",
  "main": "out-tsc/index.js",
  "typings": "out-tsc/index.d.ts",
  "scripts": {
    "build": "npm run build:tsc && npm run build:css && npm run build:copyAssets",
    "build:copyAssets": "cp -R src/*.scss ./out-tsc && cp -R src/*.css ./out-tsc && cp -R src/*html ./out-tsc",
    "build:tsc": "tsc -p src/tsconfig.app.json",
    "build:css": "node-sass src -o src",
    "deploy": "npm run build && ng github-pages:deploy",
    "clean": "rm -rf out-tsc && find src -name '*.css' -delete",
    "e2e": "protractor",
    "example": "npm run watch & ng serve",
    "lint": "npm run lint:src && npm run lint:example",
    "lint:src": "tslint \"src/**/*.ts\"",
    "lint:example": "tslint \"example/**/*.ts\"",
    "prebuild": "npm run clean",
    "pree2e": "webdriver-manager update",
    "prepublish": "npm run build",
    "postpublish": "npm run clean",
    "test": "ng test",
    "start": "npm run example",
    "watch": "npm run watch:css",
    "watch:css": "node-sass -w src -o src",
    "watch:tsc": "tsc -w -p src/tsconfig.app.json"
  },
  "private": true,
  "peerDependencies": {
    "@angular/common": "^4.0.0",
    "@angular/compiler": "^4.0.0",
    "@angular/core": "^4.0.0",
    "@angular/forms": "^4.0.0",
    "@angular/http": "^4.0.0",
    "@angular/platform-browser": "^4.0.0",
    "@angular/platform-browser-dynamic": "^4.0.0",
    "@angular/router": "^4.0.0",
    "core-js": "^2.4.1",
    "d3": "^4.7.4",
    "rxjs": "^5.1.0",
    "venn.js": "^0.2.14",
    "zone.js": "^0.8.4"
  },
  "devDependencies": {
    "@angular/cli": "1.0.0",
    "@angular/common": "^4.0.0",
    "@angular/compiler": "^4.0.0",
    "@angular/compiler-cli": "^4.0.0",
    "@angular/core": "^4.0.0",
    "@angular/forms": "^4.0.0",
    "@angular/http": "^4.0.0",
    "@angular/platform-browser": "^4.0.0",
    "@angular/platform-browser-dynamic": "^4.0.0",
    "@angular/router": "^4.0.0",
    "@types/jasmine": "2.5.38",
    "@types/node": "~6.0.60",
    "codelyzer": "~2.0.0",
    "core-js": "^2.4.1",
    "d3": "^4.7.4",
    "jasmine-core": "~2.5.2",
    "jasmine-spec-reporter": "~3.2.0",
    "karma": "~1.4.1",
    "karma-chrome-launcher": "~2.0.0",
    "karma-cli": "~1.0.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-coverage-istanbul-reporter": "^0.2.0",
    "protractor": "~5.1.0",
    "rxjs": "^5.1.0",
    "ts-node": "~2.0.0",
    "tslint": "~4.5.0",
    "typescript": "~2.2.0",
    "venn.js": "^0.2.14",
    "zone.js": "^0.8.4"
  }
}
